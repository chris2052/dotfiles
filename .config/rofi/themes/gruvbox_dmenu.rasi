configuration{
	show-icons: 				false;
}
* {
    background-color:           #303030;
    border-color:               #282c34;
    text-color:                 #bbc2cf;
    height:                     20px;
    font:                       "CaskaydiaCove Nerd Font Medium 9";
    prompt-font:                "CaskaydiaCove Nerd Font Medium 10";
	show-icons: 				false;
    prompt-background:          #51b0ef81;
    prompt-foreground:          #2e2c2c;
    prompt-padding:             2px;
    selected-normal-background: #ae403e8f;
    selected-normal-foreground: #ffffffec;
}
#window {
    anchor: north;
    location: north;
    width: 100%;
    padding: 0px;
    children: [ horibox ];
}
#horibox {
    orientation: horizontal;
    children: [ prompt, entry, listview ];
}
#prompt {
    padding:          @prompt-padding;
    background-color: @prompt-background;
    text-color:       @prompt-foreground;
    font:             @prompt-font;
}
#listview {
    layout: horizontal;
    lines: 100;
}
#entry {
    padding: 2px;
    expand: false;
    width: 15em;
}
#element {
    padding: 2px 8px;
}
#element selected {
    background-color: @selected-normal-background;
    text-color:       @selected-normal-foreground;
}

element-text {
    background-color: inherit;
    text-color:       inherit;
}